{
  "name": "@island.is/regulations-api-server",
  "version": "1.0.0",
  "author": "Hugsmiðajn ehf. (www.hugsmidjan.is)",
  "contributors": [
    "Már Örlygsson <mar@hugsmidjan.is>",
    "Valur Sverrisson <valur@hugsmidjan.is>"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit  &&  NODE_ENV=production node build.js",
    "dev-watch": "yarn  &&  yarn concurrently \"tsc --noEmit --watch\"  \"node build.js --watch\"",
    "dev-api-server": "yarn nodemon -r dotenv/config dist/server.js --ignore 'backup-json/*'",
    "dev-proxy-server": "yarn nodemon -r dotenv/config dist/proxyServer.js --ignore 'backup-json/*'",
    "dev-proxy": "node build.js  &&  yarn concurrently -k \"yarn dev-watch\" \"yarn dev-proxy-server\"",
    "dev-api": "node build.js  &&  yarn concurrently -k \"yarn dev-watch\" \"yarn dev-api-server\"",
    "dev": "node build.js  &&  yarn concurrently -k \"yarn dev-watch\" \"yarn dev-api-server\" \"yarn dev-proxy-server\"",
    "script": "node -r dotenv/config",
    "//start": "# Þetta er gert í flýti, mætti gera betur svo hægt sé að keyra tvö heroku apps frá sama codebase",
    "start-api": "NODE_ENV=production node -r dotenv/config dist/server.js",
    "start-proxy": "NODE_ENV=production node -r dotenv/config dist/proxyServer.js"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.15.0",
    "@hugsmidjan/qj": "^4.6.1",
    "@hugsmidjan/regulations-editor": "*",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.1",
    "@types/jsdom": "^16.2.9",
    "@types/multer-s3-transform": "npm:@types/multer-s3@^2.7.10",
    "@types/node": "^16.11.7",
    "@types/node-fetch": "^2.5.10",
    "@types/sharp": "^0.29.2",
    "aws-sdk": "^2.1018.0",
    "date-fns": "^2.25.0",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "elastic-builder": "^2.13.0",
    "esbuild": "^0.14.2",
    "fastify": "^3.11.0",
    "fastify-basic-auth": "^2.2.0",
    "fastify-compress": "^3.4.2",
    "fastify-elasticsearch": "^2.0.0",
    "fastify-http-proxy": "^6.2.1",
    "fastify-multer": "^2.0.2",
    "fastify-multipart": "^5.1.0",
    "fastify-rate-limit": "^5.0.1",
    "fastify-redis": "^4.3.3",
    "file-type": "^16.5.3",
    "htmldiff-js": "^1.0.5",
    "jsdom": "^16.5.2",
    "multer-s3-transform": "^2.10.3",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "pagedjs-cli": "^0.1.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "sharp": "^0.29.2",
    "typescript": "^4.4.4",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "concurrently": "^6.0.1",
    "nodemon": "^2.0.15"
  }
}
